                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="1px" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>Installing the <fo:inline>RMF for z/OS</fo:inline> plugin in
                                                  the Grafana stand-alone application.</fo:block></fo:table-cell>
                                                  <fo:table-cell start-indent="0" end-indent="0" border-bottom-style="solid" border-bottom-width="1px" border-right-style="solid" border-right-width="0" padding-bottom="0.3em" padding-left="0.3em" padding-right="0.3em" padding-top="0.3em" display-align="before"><fo:block>Perform steps <fo:inline color="rgb(35 , 114 , 222)"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_zdv_jqy_g1c">3</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_zdv_jqy_g1c"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_zdv_jqy_g1c"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_zdv_jqy_g1c"><fo:page-number-citation ref-id="unique_6_Connect_42_step_zdv_jqy_g1c"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_zdv_jqy_g1c">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline>
                                                  and <fo:inline color="rgb(35 , 114 , 222)"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_dbx_4qy_g1c">4</fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_dbx_4qy_g1c"><fo:inline font-style="italic"><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_dbx_4qy_g1c"> (on page </fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_dbx_4qy_g1c"><fo:page-number-citation ref-id="unique_6_Connect_42_step_dbx_4qy_g1c"/></fo:basic-link><fo:basic-link show-destination="new" indicate-destination="true" internal-destination="unique_6_Connect_42_step_dbx_4qy_g1c">)</fo:basic-link></fo:inline></fo:basic-link></fo:inline>.</fo:block></fo:table-cell>
